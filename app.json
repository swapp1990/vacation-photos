{
  "expo": {
    "name": "Vacation Photos",
    "slug": "vacation-photos",
    "version": "1.1.0",
    "scheme": "vacationphotos",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/appstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/vacation-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#4AC4F3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swapp1990.vacationphotos",
      "buildNumber": "7",
      "associatedDomains": [
        "applinks:swapp1990.github.io",
        "appclips:swapp1990.github.io"
      ],
      "entitlements": {
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.swapp1990.vacationphotos"
        ],
        "com.apple.developer.icloud-services": [
          "CloudKit"
        ],
        "com.apple.security.application-groups": [
          "group.com.swapp1990.vacationphotos"
        ]
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos to display and organize your vacation memories.",
        "NSLocationWhenInUseUsageDescription": "This app needs your location to identify photos taken away from home.",
        "NSContactsUsageDescription": "This app needs access to your contacts to share vacation photos with friends and family.",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "whatsapp",
          "sms"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcons/appstore.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withCloudKit",
      "./plugins/withAppClip"
    ],
    "extra": {
      "eas": {
        "projectId": "bae687ee-1832-4ba2-a0d3-cc61d4feaffa"
      }
    },
    "owner": "swapp1990"
  }
}
